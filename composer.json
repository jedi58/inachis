{
    "name": "jedi58/inachis",
    "type": "project",
    "description": "A basic framework for use in producing simple post/page driven websites",
    "license": "proprietary",
    "authors": [
        { "name": "David G. Paul" }
    ],
    "version": "0.0.5",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jedi58/versioning-bundle"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-iconv": "*",
        "ext-json": "*",
        "aptoma/twig-markdown": "^3.0",
        "beberlei/doctrineextensions": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.11.1",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "jaybizzle/crawler-detect": "^1.2",
        "jedi58/versioning-bundle": "5.0.1",
        "jfcherng/php-diff": "^6.4",
        "michelf/php-markdown": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.2",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.0",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/flex": "^2.3",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "6.*",
        "symfony/http-kernel": "^7.0",
        "symfony/mailer": "7.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/password-hasher": "7.*",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/rate-limiter": "7.*",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "*",
        "pheromone/phpcs-security-audit": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^9.0",
        "psalm/phar": "^4.23",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/error-handler": "^7.0",
        "symfony/maker-bundle": "^1.51",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "create-skeleton": [
            "cp -R vendor/jedi58/inachis/{bin,config,public} .",
            "cp -R vendor/jedi58/inachis/.env.dist .env"
        ],
        "post-create-project-cmd": [
            "@create-skeleton",
            "@auto-scripts"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C425H726J8GJGXQBKPRWG2F5",
            "allow-contrib": true,
            "docker": true,
            "require": "7.*"
        }
    }
}
